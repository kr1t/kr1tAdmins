{
  "name": "kr1tAdmin",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.7.0",
    "@japa/preset-adonis": "^1.1.0",
    "@japa/runner": "^2.0.9",
    "adonis-preset-ts": "^2.1.0",
    "console-log-colors": "^0.2.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "pino-pretty": "^8.0.0",
    "prettier": "^2.7.1",
    "typescript": "~4.6",
    "youch": "^3.2.0",
    "youch-terminal": "^2.1.4"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.2.1",
    "@adonisjs/core": "^5.8.3",
    "@adonisjs/i18n": "^1.5.6",
    "@adonisjs/lucid": "^18.0.1",
    "@adonisjs/redis": "^7.3.0",
    "@adonisjs/repl": "^3.1.11",
    "@formkit/vue": "^1.0.0-beta.8",
    "axios": "^0.27.2",
    "barcode": "^0.1.0",
    "generate-placeholder-image": "^1.1.0",
    "gradient-string": "^2.0.1",
    "html-pdf": "^3.0.1",
    "luxon": "^2.4.0",
    "mysql": "^2.18.1",
    "phc-argon2": "^1.1.3",
    "proxy-addr": "^2.0.7",
    "qrcode": "^1.5.0",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "stream": "^0.0.2",
    "string-crypto": "^2.0.2"
  }
}
